  <div class="guitar">
  <h1 class="head-border">Guitar</h1>
  <ul class="card-list">
    <% @posts.each do |post| %>
    <li class="card">
      <%= link_to image_tag(post.image, class:"google-btn"), post_path(post.id) %>
        <p class="inst-title"><%= post.title %></p>
        </li>
    <% end %>
  </div>